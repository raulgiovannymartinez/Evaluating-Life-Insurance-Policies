    "    except:\n",
    "        continue\n",
    "        \n",
    "    if i == 0:\n",
    "        tota_value_all_policies = np.zeros(len(V_temp.valuation))\n",
    "        i+=1\n",
    "    \n",
    "    tota_value_all_policies+=V_temp.valuation"
